// app global css
@import './main.css';
@import './animations/index.css';

// fade in
.fade-enter{
    opacity: 0;
}
.fade-enter-active{
    transition: all linear .5s;
}
.fade-enter-to{
    opacity: 1;
}
.fade-leave{
    opacity: 1;
}
.fade-leave-active{
    transition: all linear .5s;
}
.fade-leave-to{
    opacity: 0;
}
